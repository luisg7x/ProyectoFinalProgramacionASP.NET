@model ProyectoP5.Models.EmpresaCRUDModel
<footer class="footer-distributed">
    <div class="footer-left">
        <h3>Gaming PC</h3>
        <p class="footer-links">
            <a href="@Url.Action("Index", "Inicio")">Inicio</a>       
            ·
            <a href="@Url.Action("Contact", "Inicio")">Contacto</a>
            ·
            <a href='@Url.Action("About", "Inicio")'>Acerca de Nosotros</a>
        </p>
        <p class="footer-company-name">&copy;Todos los Derechos Reservados 2015 - 2017</p>
    </div>
    <div class="footer-center">
        <div>
            <i class="fa fa-map-marker"></i>
            <p>@Html.DisplayFor(model => model.DireccionEmpresa)</p>
        </div>
        <div>
            <i class="fa fa-phone"></i>
            <p>@Html.DisplayFor(model => model.TelefonoEmpresa)</p>
        </div>
        <div>
            <i class="fa fa-envelope"></i>
            <p><a href="mailto:gamingPC@gmail.com">@Html.DisplayFor(model => model.CorreoEmpresa)</a></p>
        </div>
    </div>
    <div class="footer-right">
        <p class="footer-company-about">
            <span>Acerca de la compañia</span>
            Somos una compañia especializada en la venta de componentes Gaming.
        </p>
        <div class="footer-icons">
            <a href="#"><i class="icon-facebook"></i></a>
            <a href="#"><i class="icon-twitter"></i></a>
            <a href="#"><i class="icon-linkedin"></i></a>
            <a href="#"><i class="icon-google-plus"></i></a>
        </div>
    </div>
</footer>
